/*******************************
         Site Overrides
*******************************/

.ui.borderless.menu {
  box-shadow: none;
  border: none;
}
.ui.borderless.menu > .item:first-child, .ui.borderless.menu > .item:last-child{
  border-radius: 0;
}

/*------------------------------
     remove Hover Effect
------------------------------*/

.ui.menu .item.noHover:hover {
  background-color: transparent !important;
  color:red;
}
